{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alexandrubau/messenger-azure-queue-transport": "^1.1",
        "api-platform/core": "^3.0",
        "aws/aws-sdk-php": "^3.228",
        "aymdev/messenger-azure-bundle": "^1.4",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/orm": "^2.13",
        "nelmio/cors-bundle": "^2.3",
        "nyholm/psr7": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.16",
        "sendgrid/sendgrid": "^8.0",
        "symfony/amazon-mailer": "5.4.*",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.*",
        "symfony/console": "5.4.*",
        "symfony/doctrine-messenger": "6.*",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "6.*",
        "symfony/flex": "^1.17|^2",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/messenger": "6.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/property-access": "6.*",
        "symfony/property-info": "6.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "6.*",
        "symfony/sendgrid-mailer": "5.4.*",
        "symfony/serializer": "6.*",
        "symfony/twig-bundle": "6.*",
        "symfony/validator": "6.*",
        "symfony/yaml": "5.4.*",
        "webonyx/graphql-php": "^15.2"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.*",
        "symfony/css-selector": "6.*",
        "symfony/maker-bundle": "^1.43",
        "symfony/phpunit-bridge": "^6.2"
    }
}
